   label="Teilen..."
   name="share">
     <menu_item_call.on_click
      function="Profile.Action"
      parameter="share"/>
     <menu_item_call.on_enable
      function="Profile.Enable"
      parameter="can_share" />
  </menu_item_call>
  <menu_item_call
   label="Anrufen..."
   name="call">
