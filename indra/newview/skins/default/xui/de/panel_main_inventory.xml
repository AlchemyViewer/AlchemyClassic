		<layout_panel name="add_btn_panel">
			<button name="add_btn" tool_tip="Neues Objekt hinzufügen"/>
		</layout_panel>
		<layout_panel name="trash_btn_panel">
			<dnd_button name="trash_btn" tool_tip="Auswahl löschen"/>
