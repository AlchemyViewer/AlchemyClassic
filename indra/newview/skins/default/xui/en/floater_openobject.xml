<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="18"
 can_resize="true"
 default_tab_group="1"
 height="322"
 layout="topleft"
 min_height="160"
 min_width="285"
 name="objectcontents"
 help_topic="objectcontents"
 save_rect="true"
 title="OBJECT CONTENTS"
 width="300">
   <text
     type="string"
     length="1"
     follows="left|top|right"
     font="SansSerif"
     text_color="White"
     height="16"
     layout="topleft"
     left="10"
     name="object_name"
     parse_urls="false"
     top="20"
     width="284">
        [DESC]:
    </text>
    <panel_inventory_object
     background_visible="false"
     draw_border="false"
     follows="all"
     height="250"
     layout="topleft"
     left="10"
     name="object_contents"
     top_pad="0"
     width="284" />

    <flyout_button
     follows="left|right|bottom"
     height="23"
     label="Copy To Inventory"
     layout="topleft"
     left="10"
     name="copy_flyout"
     top_pad="5"
     width="153">
        <flyout_button.item
         label="Replace outfit with contents"
         name="replace"
         value="replace" />
        <flyout_button.item
         label="Add contents to outfit"
         name="add"
         value="add" />
    </flyout_button>
    <button
     follows="bottom|right"
     height="23"
     label="Cancel"
     label_selected="Cancel"
     layout="topleft"
     left_pad="5"
     name="cancel_button"
     width="125">
        <button.commit_callback
         function="OpenObject.Cancel" />
    </button>
</floater>
