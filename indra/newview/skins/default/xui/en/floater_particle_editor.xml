<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 can_resize="true"
 height="357"
 min_height="359"
 min_width="480"
 name="particle_editor_floater"
 save_rect="true"
 save_visibility="false"
 single_instance="false"
 title="PARTICLE EDITOR"
 width="480">
