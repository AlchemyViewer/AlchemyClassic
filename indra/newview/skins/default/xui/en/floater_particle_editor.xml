           follows="all"
           height="152"
           left="0"
           name="burst_panel"
           top="0"
        	  right="-1">
            <slider
             follows="left|top"
             decimal_digits="2"
             initial_value="0.3"
             increment="0.1"
             name="burst_rate"
             label="Rate:"
             label_width="50"
             can_edit_text="true"
             left="0"
             width="275"
             min_val="0"
             height="16"
             max_val="1000"
             top="0">
              <slider.commit_callback
               function="Particle.Edit" />
            </slider>
            <slider
