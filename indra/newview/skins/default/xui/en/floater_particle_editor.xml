   follows="all"
   animate="false"
   top="0"
   height="357"
   left="0"
   right="-1"
   drag_handle_gap="0"
   drag_handle_first_indent="1"
   drag_handle_second_indent="1"
   layout="topleft"
   name="master_stack"
   orientation="horizontal"
   show_drag_handle="true">
