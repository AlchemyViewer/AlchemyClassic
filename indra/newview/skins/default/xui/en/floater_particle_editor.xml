     bg_alpha_color="DkGray2"
     bg_opaque_color="DkGray2"
     background_visible="true"
     background_opaque="true"
     name="layout_master"
     follows="all"
     layout="topleft"
     auto_resize="false"
     user_resize="false"
     height="357"
     width="300">
