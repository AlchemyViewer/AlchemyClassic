<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="left|top|right|bottom"
 height="408"
 label="Communication"
 layout="topleft"
 left="102"
 name="im"
 top="1"
 width="517">
<tab_container
 label="Alchemy"
 layout="topleft"
 follows="all"
 top="5"
 bottom="-10"
 left="1"
 right="-1"
 name="tabs"
 tab_min_width="50"
 tab_position="top" >
<!--Alchemy Command Line-->
  <panel
      top_pad="5"
      bottom="-1"
      left="1"
      right="-1"
      follows="all"
      label="Chat Commands"
      name="CommandLineTab" >
    <check_box
      top="10"
      follows="left|top"
      height="16"
      label="Enable chat bar commands"
      left="5"
      name="chat_cmd_toggle"
      width="270"
      control_name="AlchemyChatCommandEnable"/>.
    <!--Draw Distance-->
    <text
      top_pad="5"
      follows="left|top"
      height="16"
      name="AlchemyChatCommandDrawDistance_txt"
      width="250">
      Change Draw Dist (usage: cmd meters)
    </text>
    <line_editor
      top_pad="0"
      follows="left|top"
      height="20"
      max_length_chars="256"
      name="AlchemyChatCommandDrawDistance"
      tool_tip=""
      control_name="AlchemyChatCommandDrawDistance"
      width="200"/>
    <!--Ground-->
    <text
      top_pad="5"
      follows="left|top"
      height="16"
      name="AlchemyChatCommandGround_txt"
      width="250">
      Teleport to ground
    </text>
    <line_editor
      top_pad="0"
      follows="left|top"
      height="20"
      max_length_chars="256"
      name="AlchemyChatCommandGround"
      tool_tip=""
      control_name="AlchemyChatCommandGround"
      width="200"/>
    <!--Rez Platform-->
    <text
      top_pad="5"
      follows="left|top"
      height="16"
      name="AlchemyChatCommandRezPlat_txt"
      width="250">
      Rez a platform (usage: cmd 0 - 30)
    </text>
    <line_editor
      top_pad="0"
      follows="left|top"
      height="20"
      max_length_chars="256"
      name="AlchemyChatCommandRezPlat"
      tool_tip=""
      control_name="AlchemyChatCommandRezPlat"
      width="200"/>
    <slider
      top_pad="4"
      name="AlchemyChatCommandRezPlatSize"
      control_name="AlchemyChatCommandRezPlatSize"
      decimal_digits="1"
      follows="left|top"
      height="18"
      increment="0.5"
      initial_value="20"
      label="Width"
      label_width="30"
      max_val="30"
      min_val="5"
      show_text="true"
      width="200"
      tool_tip="How big do you want the platform to appear to be?"/>

    <view_border
      bevel_style="none"
      border_thickness="0"
      top="25"
      left="240"
      bottom="-25"
      width="0"
      follows="top|left|bottom"
      name="EmDivisor"/>
    <!--key2name-->
    <text
      top="25"
      follows="left|top"
      height="16"
      left_delta="10"
      name="AlchemyChatCommandHeight_txt"
      width="270">
      Teleport to specified height (0-8191)
    </text>
    <line_editor
      top_pad="0"
      follows="left|top"
      height="20"
      max_length_chars="256"
      name="AlchemyChatCommandHeight"
      tool_tip=""
      control_name="AlchemyChatCommandHeight"
      width="200"/>
    <!--cao-->
    <text
      top_pad="5"
      follows="left|top"
      height="16"
      name="AlchemyChatCommandHome_txt"
      width="250">
      Teleport Home
    </text>
    <line_editor
      top_pad="0"
      follows="left|top"
      height="20"
      max_length_chars="256"
      name="AlchemyChatCommandHome"
      tool_tip=""
      control_name="AlchemyChatCommandHome"
      width="200"/>
  </panel>
  </tab_container>
</panel>
