  <panel
   label="Autoresponse"
   border="false"
   height="500"
   layout="topleft"
   name="Autoresponse"
   top="0"
   left="13"
   width="517">
    <text
     type="string"
     length="1"
     follows="left|top"
     height="13"
     layout="topleft"
     left="10"
     mouse_opaque="false"
     name="dnd_box"
     top_pad="3"
     width="240">
       Do Not Disturb response:
    </text>
    <text_editor
     control_name="DoNotDisturbModeResponse"
     text_readonly_color="TextFgReadOnlyColor"
     bg_writeable_color="TextBgWriteableColor"
     use_ellipses="false"
     commit_on_focus_lost="true"
     follows="left|top"
     height="60"
     layout="topleft"
     left="15"
     name="do_not_disturb_response"
     right="-15"
     word_wrap="true"
     max_length_bytes="1024">
       log_in_to_change
    </text_editor>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="13"
     layout="topleft"
     left="10"
     mouse_opaque="false"
     name="autoresponse_box"
     top_pad="3"
     width="240">
       Autoresponse:
    </text>
    <text_editor
     control_name="AlchemyAutoresponse"
     text_readonly_color="TextFgReadOnlyColor"
     bg_writeable_color="TextBgWriteableColor"
     use_ellipses="false"
     commit_on_focus_lost="true"
     follows="left|top"
     height="60"
     layout="topleft"
     left="15"
     name="AlchemyAutoresponse"
     right="-15"
     word_wrap="true"
     max_length_bytes="1024">
       log_in_to_change
    </text_editor>
    <text
     type="string"
     length="1"
     follows="left|top"
     height="13"
     layout="topleft"
     left="10"
     mouse_opaque="false"
     name="autoresponse_nf_box"
     top_pad="3"
     width="240">
       Autoresponse to Non-Friends:
    </text>
