<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 height="408"
 label="Text Chat"
 layout="topleft"
 name="general_chat_settings"
 width="517">
  <tab_container
   layout="topleft"
   follows="all"
   top="5"
   bottom="-1"
   name="tab_container"
   tab_min_width="50"
   tab_position="top">
  <panel
   label="Chat"
   border="false"
   height="380"
   layout="topleft"
   top="0"
   left="13"
   width="517"
   name="chat_tab">
    <check_box
     control_name="PlayTypingAnim"
     height="16"
     initial_value="true"
     label="Play typing animation when chatting"
     layout="topleft"
     top_pad="10"
     left="5"
     name="play_typing_animation"
     width="150"/>
    <check_box
     control_name="PlayChatAnim"
     height="16"
     initial_value="true"
     label="Play shout/whisper animations when chatting"
     layout="topleft"
     top_pad="6"
     left="5"
     name="PlayChatAnim"
     width="150"/>
    <check_box
     control_name="AlchemyNearbyTypingIndicators"
     height="16"
     initial_value="true"
     label="Show nearby chat indicators"
     layout="topleft"
     top_pad="6"
     left="5"
     name="show_nearby_chat_indicators"
     width="150"/>
    <check_box
     follows="left|top"
     height="16"
     label="Announce incoming instant messages"
     tool_tip="Notifies you of incoming messages by monitoring typing start messages"
     left_delta="0"
     top_pad="6"
     name="announce_im_toggle"
     width="275"
     control_name="AlchemyNotifyIncomingMessage"/>
    <check_box
     follows="left|top"
     height="16"
     label="Don&apos;t send typing notification in IM"
     tool_tip="This won't show your correspondent &quot;[name] is typing...&quot;"
     left_delta="0"
     top_pad="6"
     name="typing_notifications_toggle"
     width="275"
     control_name="AlchemyIMHideTyping"/>
    <check_box
     control_name="ChatAutocompleteGestures"
     height="16"
     initial_value="true"
     label="Auto-complete gestures in nearby chat"
     follows="left|top"
     layout="topleft"
     left_delta="0"
     top_pad="6"
     name="auto_complete_gestures"
     width="330">
    </check_box>
    <check_box
     follows="left|top"
     height="16"
     label="Automatically close chat bar when pressing Enter"
     tool_tip="Will only close the torn off (detached) chat bar and window, not the CHUI floater."
     left_delta="0"
     top_pad="6"
     name="chat_auto_close"
     width="275"
     control_name="CloseChatOnReturn"/>
    <check_box
     follows="left|top"
     height="16"
     label="Enable Ctrl+Enter to shout"
     tool_tip="Enables shouting with the Ctrl+Enter key combination."
     left_delta="0"
     top_pad="6"
     name="close_chat_on_return_toggle"
     width="275"
     control_name="AlchemyEnableKeyboardShout"/>
    <check_box
     follows="left|top"
     height="16"
     label="Enable Shift+Enter to Whisper"
     tool_tip="Enables whispering with the Shift+Enter key combination."
     left_delta="0"
     top_pad="6"
     name="shift_whisper_toggle"
     width="275"
     control_name="AlchemyEnableKeyboardWhisper"/>
    <check_box
     enabled="false"
     height="16"
     label="Forward offline IMs to my E-mail"
     layout="topleft"
     name="send_im_to_email"
     left_delta="0"
     top_pad="6"
     width="330"/>
    <check_box
     control_name="AlchemyChatSeconds"
     height="16"
     label="Show seconds in chatlog timestamps"
     layout="topleft"
     name="AlchemyChatSeconds"
     left_delta="0"
     top_pad="6"
     width="350"/>
    <check_box
     control_name="UseLegacyLogNames"
     height="16"
     label="Use legacy log naming conventions"
     layout="topleft"
     name="UseLegacyLogNames"
     left_delta="0"
     top_pad="6"
     width="350"/>
    <check_box
     control_name="AlchemyChatColorManager"
     height="16"
     label="Color chat names based on name tags"
     layout="topleft"
     name="username_tag_color"
     left_delta="0"
     top_pad="6"
     width="350"/>

    <panel
     follows="all"
     height="175"
     left="300"
     top="-391"
     width="212"
     border="false"
     name="chat_appearance">
    <text
        layout="topleft"
        left="0"
        height="12"
        name="chat_appearance_text"
        width="60"
        top_pad="20">
        Font Size:
    </text>
    <combo_box
        control_name="ChatFontSize"
        height="20"
        layout="topleft"
        left_pad="32"
        name="pass_combo"
        top_delta="-1"
        width="120">
        <combo_box.item
         label="Monospace"
         name="Monospace Font"
         value="3" />
        <combo_box.item
         label="Small"
         name="Small Font"
         value="0" />
        <combo_box.item
         label="Medium"
         name="Medium Font"
         value="1" />
        <combo_box.item
         label="Large"
         name="Large Font"
         value="2" />
    </combo_box>
    <text
        left="0"
        top_pad="3"
        follows="top|left"
        layout="topleft"
        width="92"
        use_ellipses="false"
        name="nearby_input_text"
        tool_tip="text">
         Nearby Input:
      </text>
    <combo_box
        control_name="NearbyChatInput"
        height="20"
        layout="topleft"
        left_pad="0"
        name="nearby_input_combo"
        top_delta="-1"
        width="120">
        <combo_box.item
         label="Chat bar"
         name="chatbar"
         value="0" />
        <combo_box.item
         label="Conversations"
         name="conversations"
         value="1" />
    </combo_box>
    <text
        left="0"
        top_pad="3"
        follows="top|left"
        layout="topleft"
        width="92"
        use_ellipses="false"
        name="nearby_output_text"
        tool_tip="text">
         Nearby Output:
      </text>
    <combo_box
        control_name="AlchemyNearbyChatOutput"
        height="20"
        layout="topleft"
        left_pad="0"
        name="nearby_output_combo"
        top_delta="-1"
        width="120">
        <combo_box.item
         label="Toasts"
         name="toast"
         value="0" />
        <combo_box.item
         label="Bubble"
         name="bubble"
         value="1" />
        <combo_box.item
         label="Toasts and Bubble"
         name="toast_and_bubble"
         value="2" />
        <combo_box.item
         label="None"
         name="nearby_output_none"
         value="3" />
    </combo_box>
    <text
        left="0"
        top_pad="3"
        follows="top|left"
        layout="topleft"
        width="141"
        use_ellipses="false"
        name="test_text"
        tool_tip="text">
         Nearby Toast Width:
      </text>
      <spinner
        left_pad="16"
        top_delta="-1"
        height="10"
        width="55"
        decimal_digits="0"
        increment="1"
        min_val="300"
        max_val="1500"
        follows="top|left"
        layout="topleft"
        name="alchemynearbychattoastwidth"
        control_name="AlchemyNearbyChatToastWidth"
        tool_tip="Controls the width of the nearby chat toast (pop-up)"/>


    </panel>
    
    <text
        layout="bottomleft"
        text_color="White"
        height="17"
        left="10"
        bottom="68"
        width="55">
        Save:
    </text>

    <combo_box
        enabled="false"
        control_name="KeepConversationLogTranscripts"
        height="23"
        layout="topleft"
		follows="left|bottom"
        name="chat_font_size"
        left_pad="20"
		bottom_delta="3"
        width="165">
        <item
            label="Log and transcripts"
            value="2"/>
        <item
            label="Log only"
            value="1"/>
        <item
            label="No log or transcripts"
            value="0"/>
    </combo_box>

    <button
        enabled="false"
        height="23"
        label="Clear log..."
        layout="topleft"
		follows="left|bottom"
        left_pad="5"
        bottom_delta="0"
        name="clear_log"
        width="90">
        <commit_callback
            function="Pref.ClearLog" />
    </button>

    <button
        enabled="false"
        height="23"
        label="Delete transcripts..."
        layout="topleft"
        left_pad="5"
        bottom_delta="0"
        name="delete_transcripts"
        width="145">
        <button.commit_callback
            function="Pref.DeleteTranscripts" />
    </button>

    <text
        layout="bottomleft"
        left="10"
        text_color="White"
        height="12"
        bottom="45"
        width="55">
        Location:
    </text>

    <line_editor
        control_name="InstantMessageLogPath"
        border_style="line"
        border_thickness="1"
        font="SansSerif"
        height="23"
        layout="topleft"
        left_pad="20"
        text_pad_left="5"
        max_length_bytes="4096"
        name="log_path_string"
        top_delta="-5"
        width="312">
    </line_editor>

    <button
        enabled="false"
        height="23"
        label="Browse..."
        label_selected="Browse"
        layout="topleft"
        left_pad="5"
        name="log_path_button"
        bottom_delta="0"
        width="95">
      <commit_callback function="Pref.LogPath" />
    </button>
    <button
      height="23"
      label="Translation..."
      layout="bottomleft"
      left="10"
      name="ok_btn"
      bottom="5"
      width="159">
    <commit_callback
        function="Pref.TranslationSettings" />
  </button>
  <button
      height="23"
      layout="topleft"
      top_pad="-23"
      left_pad="5"
      name="autoreplace_showgui"
      commit_callback.function="Pref.AutoReplace"
      label="Auto-Replace..."
      width="159">
  </button>
  <button
      height="23"
      layout="topleft"
      top_pad="-23"
      left_pad="5"
      name="spellcheck_showgui"
      commit_callback.function="Pref.SpellChecker"
      label="Spell Checking..."
      width="159">
  </button>
  </panel><!--End of chat tab-->
