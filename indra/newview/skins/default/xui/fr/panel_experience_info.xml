			<layout_stack>
				<layout_panel>
					<text name="experience_title" value="Kyle’s Superhero RPG"/>
				</layout_panel>
				<layout_panel name="location panel">
					<text name="Location">
						Endroit :
					</text>
					<text name="LocationTextText">
						quelque part
					</text>
					<button label="Téléportation" name="teleport_btn"/>
